    Name = "‡πÄ‡∏°‡∏ô‡∏π‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤"
})

SettingsTab:AddButton({
    Name = "‡πÇ‡∏´‡∏°‡∏î‡∏Å‡∏•‡∏≤‡∏á‡∏ß‡∏±‡∏ô",
    Default = false,
    Callback = function()
        -- ‡πÄ‡∏•‡πà‡∏ô‡πÄ‡∏™‡∏µ‡∏¢‡∏á‡πÄ‡∏°‡∏∑‡πà‡∏≠‡∏Å‡∏î‡∏õ‡∏∏‡πà‡∏°
        playButtonClickSound()

        local lighting = game:GetService("Lighting")
        lighting.ClockTime = 12
        lighting.Changed:Connect(function()
            lighting.ClockTime = 12
        end)
        ShowNotification("‡πÄ‡∏õ‡∏¥‡∏î‡πÇ‡∏´‡∏°‡∏î‡∏Å‡∏•‡∏≤‡∏á‡∏ß‡∏±‡∏ô‡πÅ‡∏•‡πâ‡∏ß!")
    end
})

SettingsTab:AddButton({
    Name = "‡πÇ‡∏´‡∏°‡∏î‡∏Å‡∏•‡∏≤‡∏á‡∏Ñ‡∏∑‡∏ô",
    Default = false,
    Callback = function()
        -- ‡πÄ‡∏•‡πà‡∏ô‡πÄ‡∏™‡∏µ‡∏¢‡∏á‡πÄ‡∏°‡∏∑‡πà‡∏≠‡∏Å‡∏î‡∏õ‡∏∏‡πà‡∏°
        playButtonClickSound()

        local lighting = game:GetService("Lighting")
        lighting.ClockTime = 0
        lighting.Changed:Connect(function()
            lighting.ClockTime = 0
        end)
        ShowNotification("‡πÄ‡∏õ‡∏¥‡∏î‡πÇ‡∏´‡∏°‡∏î‡∏Å‡∏•‡∏≤‡∏á‡∏Ñ‡∏∑‡∏ô‡πÅ‡∏•‡πâ‡∏ß!")
    end
})

local Players = game:GetService("Players")
local player = Players.LocalPlayer

local function rejoinServer()
    local TeleportService = game:GetService("TeleportService")
    TeleportService:Teleport(game.PlaceId, player)
end

SettingsTab:AddButton({
    Name = "‡∏£‡∏µ‡∏à‡∏≠‡∏¢",
    Callback = function()
        -- ‡πÄ‡∏•‡πà‡∏ô‡πÄ‡∏™‡∏µ‡∏¢‡∏á‡πÄ‡∏°‡∏∑‡πà‡∏≠‡∏Å‡∏î‡∏õ‡∏∏‡πà‡∏°
        playButtonClickSound()

        rejoinServer()
    end
})

SettingsTab:AddButton({
    Name = "‡∏≠‡∏≠‡∏Å‡∏à‡∏≤‡∏Å‡πÄ‡∏Å‡∏°",
    Callback = function()
        -- ‡πÄ‡∏•‡πà‡∏ô‡πÄ‡∏™‡∏µ‡∏¢‡∏á‡πÄ‡∏°‡∏∑‡πà‡∏≠‡∏Å‡∏î‡∏õ‡∏∏‡πà‡∏°
        playButtonClickSound()

        game:Shutdown()
    end
})

-- ‡πÅ‡∏ó‡πá‡∏ö‡πÄ‡∏õ‡∏¥‡∏î‡πÄ‡∏û‡∏•‡∏á
local MusicTab = Window:MakeTab({
    Name = "‡πÄ‡∏õ‡∏¥‡∏î‡πÄ‡∏û‡∏•‡∏á",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

local Section = MusicTab:AddSection({
    Name = "‡πÄ‡∏°‡∏ô‡∏π‡πÄ‡∏õ‡∏¥‡∏î‡πÄ‡∏û‡∏•‡∏á"
})

-- ‡∏™‡∏£‡πâ‡∏≤‡∏á‡∏ï‡∏±‡∏ß‡πÅ‡∏õ‡∏£‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡πÄ‡∏û‡∏•‡∏á
local music = Instance.new("Sound")
music.SoundId = "rbxassetid://1843404009" -- ‡πÉ‡∏™‡πà ID ‡∏Ç‡∏≠‡∏á‡πÄ‡∏û‡∏•‡∏á‡∏ó‡∏µ‡πà‡∏ï‡πâ‡∏≠‡∏á‡∏Å‡∏≤‡∏£
music.Volume = 1
music.Looped = true
music.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")

-- Toggle ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î‡πÄ‡∏û‡∏•‡∏á
MusicTab:AddToggle({
    Name = "‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î‡πÄ‡∏û‡∏•‡∏á",
    Default = false,
    Callback = function(isPlaying)
        -- ‡πÄ‡∏•‡πà‡∏ô‡πÄ‡∏™‡∏µ‡∏¢‡∏á‡πÄ‡∏°‡∏∑‡πà‡∏≠‡∏Å‡∏î‡∏õ‡∏∏‡πà‡∏°
        playButtonClickSound()

        if isPlaying then
            music:Play()
            ShowNotification("‡πÄ‡∏õ‡∏¥‡∏î‡πÄ‡∏û‡∏•‡∏á‡πÅ‡∏•‡πâ‡∏ß!")
        else
            music:Stop()
            ShowNotification("‡∏õ‡∏¥‡∏î‡πÄ‡∏û‡∏•‡∏á‡πÅ‡∏•‡πâ‡∏ß!")
        end
    end
})
